<script>

    $(document).ready(function () {
        $("#button1").click(function () {
            $.ajax({
                url: '/GamerList/GetGamerList',
                type: 'GET',
                success: function (data) {
                    console.log(data.model);
                    load(data);
                },
                error: function (xhr) {
                    console.log("Error with remove ");
                }
            })
        });
    });



    function load(model) {
       var row = '<tr><td>' + model.model.User + '</td><td>' + model.model.Name + '</td><td>' + model.model.Agility + '</td><td>' + model.model.Defense + '</td><td>' + model.model.Strength + '</td><td>' + '<input type="button" value = "wyzwij">' + '</td></tr>';
              $row = $(row),
              resort = true;
            $('table')
              .find('tbody').append($row)
              .trigger('addRows', [$row, resort]);
            return false;
        };


</script>

